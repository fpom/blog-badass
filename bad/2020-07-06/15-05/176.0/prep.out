void buxom (char* mocha) {
    int i = 0;
    while(mocha[i] != 0){
        if(mocha[i] >= 'a'){
            if(mocha[i] == 'a' ||mocha[i] == 'e' || mocha[i] == 'i' || mocha[i] == 'o' || mocha[i] == 'u' || mocha[i] == 'y') {
                mocha[i] -= 32;
            }
        }
        i++;
    }
    return mocha;
}

